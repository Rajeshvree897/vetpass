                <div class="" id="wed-buildyourform">
                  <% if(typeof responseData==='object' && typeof responseData.VetPracticeTimeBlocks==='object' ) { 
                    for (let i=0, cnt = 1; i < responseData.VetPracticeTimeBlocks.length; i++ ) {
                      if(responseData.VetPracticeTimeBlocks[i].day === "wednesday") { %>
                        <div class="form-group wed_fieldwrapper" id="field1">
                          <label for="wednesday1" class="">Slot </label>&nbsp;<label class="slot-id wed"><%= cnt %></label>&nbsp;<label class="wed-invalid-time time-error"></label><br>
                          <div class="col-sm-2 form-inline custom-input-padding from-time">
                              <input type="hidden" name="wednesday[]" class="form-control" value="<%= responseData.VetPracticeTimeBlocks[i].id %>">
                              <input type="time" name="wednesday[]" id="wednesday1" class="form-control wednesdayTime wedTime typeTime fromtime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].from_time).format('HH:mm') %>">
                          </div>
                          <div class="col-sm-2 form-inline to-time custom-to" data-idx="<%= cnt %>">
                              <input type="time" name="wednesday[]" id="wednesday2" class="form-control wednesdayTime wedTime typeTime totime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].to_time).format('HH:mm') %>">
                          </div>
                          <input type="button" class="remove" value="-" style="position: absolute;margin-left: -2%;padding-top: 4px;padding-bottom: 4px;"><br><br>
                        </div>
                  <% cnt++ }}} %>
                </div>
                <div class="form-group row">
                  <div class="col-sm-6">
                    <input type="button" value="Add a field" class="add" id="wed-add" />
                  </div>
                </div>
              </div>
              <div class="form-group thu-div custom-slot">
                <div class="" id="thu-buildyourform">
                  <% if(typeof responseData==='object' && typeof responseData.VetPracticeTimeBlocks==='object' ) { 
                    for (let i=0, cnt = 1; i < responseData.VetPracticeTimeBlocks.length; i++ ) {
                      if(responseData.VetPracticeTimeBlocks[i].day === "thursday") { %>
                        <div class="form-group thu_fieldwrapper" id="field1">
                          <label for="thursday1" class="">Slot </label>&nbsp;<label class="slot-id thu"><%= cnt %></label>&nbsp;<label class="thu-invalid-time time-error"></label><br>
                          <div class="col-sm-2 form-inline custom-input-padding from-time">
                              <input type="hidden" name="thursday[]" class="form-control" value="<%= responseData.VetPracticeTimeBlocks[i].id %>">
                              <input type="time" name="thursday[]" id="thursday1" class="form-control thursdayTime thuTime typeTime fromtime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].from_time).format('HH:mm') %>">
                          </div>
                          <div class="col-sm-2 form-inline to-time custom-to" data-idx="<%= cnt %>">
                              <input type="time" name="thursday[]" id="thursday2" class="form-control thursdayTime thuTime typeTime totime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].to_time).format('HH:mm') %>">
                          </div>
                          <input type="button" class="remove" value="-" style="position: absolute;margin-left: -2%;padding-top: 4px;padding-bottom: 4px;"><br><br>
                        </div>
                  <% cnt++ }}} %>
                </div>
                <div class="form-group row">
                  <div class="col-sm-6">
                    <input type="button" value="Add a field" class="add" id="thu-add" />
                  </div>
                </div>
              </div>
              <div class="form-group fri-div custom-slot">
                <div class="" id="fri-buildyourform">
                  <% if(typeof responseData==='object' && typeof responseData.VetPracticeTimeBlocks==='object' ) { 
                    for (let i=0, cnt = 1; i < responseData.VetPracticeTimeBlocks.length; i++ ) {
                      if(responseData.VetPracticeTimeBlocks[i].day === "friday") { %>
                        <div class="form-group fri_fieldwrapper" id="field1">
                          <label for="friday1" class="">Slot </label>&nbsp;<label class="slot-id fri"><%= cnt %></label>&nbsp;<label class="fri-invalid-time time-error"></label><br>
                          <div class="col-sm-2 form-inline custom-input-padding from-time">
                            
                              <input type="hidden" name="friday[]" class="form-control" value="<%= parseInt(responseData.VetPracticeTimeBlocks[i].id) %>">
                              <input type="time" name="friday[]" id="friday1" class="form-control fridayTime friTime typeTime fromtime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].from_time).format('HH:mm') %>">
                          </div>
                          <div class="col-sm-2 form-inline to-time custom-to" data-idx="<%= cnt %>">
                              <input type="time" name="friday[]" id="friday2" class="form-control fridayTime friTime typeTime totime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].to_time).format('HH:mm') %>">
                          </div>
                          <input type="button" class="remove" value="-" style="position: absolute;margin-left: -2%;padding-top: 4px;padding-bottom: 4px;"><br><br>
                        </div>
                  <% cnt++ }}} %>
