                <div class="form-group row">
                  <div class="col-sm-6">
                    <input type="button" value="Add a field" class="add" id="fri-add" />
                  </div>
                </div>
              </div>
              <div class="form-group sat-div custom-slot">
                <div class="" id="sat-buildyourform">
                  <% if(typeof responseData==='object' && typeof responseData.VetPracticeTimeBlocks==='object' ) { 
                    for (let i=0, cnt = 1; i < responseData.VetPracticeTimeBlocks.length; i++ ) {
                      if(responseData.VetPracticeTimeBlocks[i].day === "saturday") { %>
                        <div class="form-group sat_fieldwrapper" id="field1">
                          <label for="saturday1" class="">Slot </label>&nbsp;<label class="slot-id sat"><%= cnt %></label>&nbsp;<label class="sat-invalid-time time-error"></label><br>
                          <div class="col-sm-2 form-inline custom-input-padding from-time">
                              <input type="hidden" name="saturday[]" class="form-control" value="<%= responseData.VetPracticeTimeBlocks[i].id %>">
                              <input type="time" name="saturday[]" id="saturday1" class="form-control saturdayTime satTime typeTime fromtime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].from_time).format('HH:mm') %>">
                          </div>
                          <div class="col-sm-2 form-inline to-time custom-to" data-idx="<%= cnt %>">
                              <input type="time" name="saturday[]" id="saturday2" class="form-control saturdayTime satTime typeTime totime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].to_time).format('HH:mm') %>">
                          </div>
