<%- include ./../layout/header.ejs -%>
<% include ./../layout/partials/fake-loader-section.ejs %>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper custom-bg">
    <!-- Content Header (Page header) -->
    <%- include ./../layout/partials/page-header-section.ejs -%>
      <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-beta.1/dist/css/select2.min.css" rel="stylesheet" />
      <!-- Main content -->
      <form action="<%= typeof responseData==='object' && responseData.id?'/admin/stores/update':'/admin/stores' %>"
        id="vetPracticesForm" method="POST" enctype="multipart/form-data">
        <div class="form-group button-row">
          <button type="submit" class="btn btn-success custom-save-btn custom-save-common custom-top save-user">Save</button>
          <a href="/admin/vetStroes" class="btn btn-primary ml-10 custom-cancel-btn backButton" data-toggle="modal" data-target="#deleteConfirmModal"></a>
          <h1 class="custom-add-heading"><%= typeof responseData === 'object' && responseData.id ? 'Edit' : 'Create' %> Store</h1>
          <h1 class="custom-add-heading-details" id="form-details-icon">How to fill in this form <i class="fa fa-question-circle"></i></h1>
          <small class="custom-add-text">API ID : store</small>
        </div>
      <section class="content">

        <!-- Default box -->
        <div class="box custom-box">
          <!-- <div class="box-header with-border">
          </div> -->
          <div class="box-body">
            <%- include ./../layout/partials/alert-section.ejs -%>

            <input type="hidden" name="id" id="id" value="<%= typeof responseData==='object'? responseData.id:''%>">
                <div class="form-group row">
                  <div class="col-sm-3">
                    <label for="practiceImage" class="">Image  (Preferred resolution - 400 x 400)</label>
                    <div class="position-relative">
                      <div class="choose-profile">
                        <input type="file" id="practiceImage" name="practiceImage" accept="image/png, image/jpeg">
                      </div>
                      <div class="d-none profile_img">
                        <img
                          src="<%= typeof responseData==='object' && responseData.img_path ? responseData.img_path: '/images/vet-practice-edit.png' %>"
                          alt="Vet Practice" height="200" width="100%" id="practice-img">
                      </div>
                    </div>
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                  <label for="practice_name" class="">Store Name</label>
                  <input type="hidden" name="option" id="option" value="store">
                    <input type="text" name="practice_name" id="practice_name" class="form-control"
                      placeholder="Store Name"
                      value="<%= typeof responseData==='object'?responseData.practice_name:'' %>">
                  </div>
                  <div class="col-sm-6">
                    <label for="public_email" class="">Public Email</label>
                      <input type="text" name="public_email" id="public_email" class="form-control"
                        placeholder="Public Email"
                        value="<%= typeof responseData==='object'?responseData.public_email:'' %>">
                    </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                    <label for="address1" class="">Address</label>
                    <input type="text" name="address1" id="address1" class="form-control" placeholder="Address 1"
                      value="<%= typeof responseData==='object'?responseData.address1:'' %>" onblur="getAddress()">
                  </div>
                  <div class="col-sm-6">
                    <label for="address2" class="">Address</label>
                    <input type="text" name="address2" id="address2" class="form-control" placeholder="Address 2"
                      value="<%= typeof responseData==='object'?responseData.address2:'' %>" onblur="getAddress()">
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                    <label for="countryId" class="">Country</label>
                    <input type="hidden" name="country" id="country"
                      value="<%= typeof responseData==='object'?responseData.country:'' %>">
                    <select class="form-control type-select-box" id="countryId"
                      value="<%= typeof responseData==='object'?responseData.id:'' %>">
                      <option <%=(typeof responseData!=='object' || !responseData.country)? 'selected' : '' %>
                        disabled>Add an item..</option>
                      <% for (var i=0; i < countryData.length; i++ ) { %>
                        <option <%=typeof responseData==='object' ?countryData[i].id===responseData.country ? 'selected'
                          : '' : '' %>
                          id="<%= countryData[i].id %>">
                            <%= countryData[i].country %>
                        </option>
                        <% } %>
                    </select>
                  </div>
                  <div class="col-sm-6">
                    <label for="stateId" class="">State/Postal/County</label>
                    <input type="hidden" name="state" id="state"
                      value="<%= typeof responseData==='object'?responseData.state:'' %>">
                    <select class="form-control type-select-box" id="stateId"
                      value="<%= typeof responseData==='object'?responseData.name:'' %>">
                      <option <%=(typeof responseData!=='object' || !responseData.state)? 'selected' : '' %>>
                        Add an
                        item..</option>
                      <% if(typeof states==='object' ) { for (var i=0; i < states.length; i++ ) { %>
                        <option <%=typeof responseData==='object' ?states[i].id===responseData.state ? 'selected' : ''
                          : '' %>
                          id="<%= states[i].id %>">
                            <%= states[i].state %>
                        </option>
                        <% } } %>
                    </select>
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                    <label for="city" class="">City/Town</label>
                    <input type="text" name="city" id="city" class="form-control" placeholder="City/Town"
                      value="<%= typeof responseData==='object'?responseData.city:'' %>">
                  </div>
                  <div class="col-sm-6">
                    <label for="zip_code" class="">Zip Code/Post Code</label>
                    <input type="Text" name="zip_code" id="zip_code" class="form-control" placeholder="Zip Code/Post Code"
                      value="<%= typeof responseData==='object'?responseData.zip_code:'' %>"  onblur="getAddress()">
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                    <label for="int_code" class="">Int Code</label>
                    <select class="form-control type-select-box" id="int_code" name="int_code">
                      <option <%= typeof responseData!=='object' || !responseData.int_code ? 'selected' : '' %>
                        disabled>Add an item..</option>
                        <% for (var i=0; i < countryData.length; i++ ) { %>
                          <% if(countryData[i].id){ %>
                          <option <%=typeof responseData==='object' ?countryData[i].id==responseData.int_code ? 'selected'
                            : '' : '' %>
                            id="<%= countryData[i].id %>" value="<%= countryData[i].id %>">
                            <%= countryData[i].country_code ? `${countryData[i].country} (+${countryData[i].country_code})` : countryData[i].country %>
                          </option>
                          <% }} %>
                    </select>
                  </div>
                  <div class="col-sm-6">
                    <label for="business_phone" class="">Business Phone</label>
                    <input type="text" name="business_phone" id="business_phone" class="form-control"
                      placeholder="Business Phone"
                      value="<%= typeof responseData==='object'?responseData.business_phone:'' %>">
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                    <label for="out_of_hr_phone" class="">Out of Hours Number</label>
                    <input type="text" name="out_of_hr_phone" id="out_of_hr_phone" class="form-control"
                    placeholder="Out of Hours Number"
                    value="<%= typeof responseData==='object'?responseData.out_of_hr_phone:'' %>">
                  </div>
                  <div class="col-sm-6">
                    <label for="email" class="">Email Address</label>
                    <input type="text" name="email" id="email" class="form-control" placeholder="Email Address"
                    value="<%= typeof responseData==='object'?responseData.email:'' %>">
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                    <label for="website" class="">Website</label>
                    <input type="text" name="website" id="website" class="form-control" placeholder="Website"
                    value="<%= typeof responseData==='object'?responseData.website:'' %>">
                  </div>
                  <div class="col-sm-6">
                    <label for="typeId" class="">Treats Categories</label>
                    <select class="form-control animal-type-multiple" id="typeId" name="types[]" multiple="multiple"
                      value="<%= typeof responseData==='object'?responseData.name:'' %>">
                      <% if(typeof animalTypes==='object' ) { for (var i=0; i < animalTypes.length; i++ ) { %>
                        <option <%=typeof responseData==='object' && responseData.VetPracticesTypeAnimal &&
                          responseData.VetPracticesTypeAnimal.includes(animalTypes[i].id) ? 'selected' : '' %>
                          value="<%= animalTypes[i].id %>">
                            <%= animalTypes[i].category %>
                        </option>
                        <% } } %>
                    </select>
                  </div>
                </div>

                <div class="form-group row">
                  <div class="col-sm-6">
                    <label for="speciality" class="">Notes, Interests and Specializations</label>
                    <select class="form-control speciality-multiple" id="speciality" name="specialityData[]" multiple="multiple">
                      <% if(typeof responseData==='object' && responseData.specialityArr ) { for (var i=0; i <
                        responseData.specialityArr.length; i++ ) { %>
                          <option selected %>
                          value="<%= responseData.specialityArr[i] %>">
                          <%= responseData.specialityArr[i] %>
                        </option>
                        <% } } %>
                      </select>
                    </div>
                    <div class="col-sm-6">
                      <label for="ClassificationId" class="">Classifications</label>
                      <select id="ClassificationId" class="classification-multiple form-control" name="storeClassification[]"
                        multiple="multiple" value="<%= typeof responseData==='object'?responseData.name:'' %>">
                        <% for (var i=0; i < classificationData.length; i++ ) { %>
                          <option <%=typeof responseData==='object' && responseData.VetPracticesStoreClassification &&
                            responseData.VetPracticesStoreClassification.includes(classificationData[i].id) ? 'selected' : ''
                            %>
                            value="<%= classificationData[i].id %>">
                              <%= classificationData[i].title %>
                          </option>
                          <% } %>
                      </select>
                    </div>
                  </div>

                  <div class="form-group row">
                    <div class="col-sm-6">
                      <label for="practice_accreditation_no" class="">Registration No</label>
                        <textarea class="form-control" rows="4" name="practice_accreditation_no" id="practice_accreditation_no"
                        placeholder="Registration No"><%= typeof responseData==='object'?responseData.practice_accreditation_no:'' %></textarea>
                    </div>
                    <!-- pet service -->
                      <div class="col-sm-6">
                        <label for="petServiceId" class="">Pet service</label>
                        <select id="petServiceId" class="pet-service-multiple form-control" name="pet_service[]"
                          multiple="multiple" value="<%= typeof responseData==='object'?responseData.name:'' %>">
                          <% for (var i=0; i < petService.length; i++ ) { %>
                            <option <%=typeof responseData==='object' && responseData.petService &&
                              responseData.petService.includes(petService[i].id) ? 'selected' : ''
                              %>
                              value="<%= petService[i].id %>">
                                <%= petService[i].name %>
                            </option>
                            <% } %>
                        </select>
                      </div>
                    <!-- <div class="col-sm-6">
                      <label for="flag" class="">Flag</label>
                      <input type="text" name="flag" id="flag" class="form-control"
                        placeholder="Flag"
                        value="<%= typeof responseData==='object'?responseData.flag:'' %>">
                    </div> -->
                  </div>

                <div class="form-group row">
                </div>

                <!-- Multiple time blocks start -->
                <div class="form-group vet-profile-hide row weekday-div<%= typeof responseData==='object'?' update-weekday':'' %>">
    
                  <div class="col-md-1 radio-div">
                    <label class="mon-lbl">
                      <input type="radio" name="weekDay" value="mon" class="card-input-element" checked />
                        <div class="panel panel-default card-input">
                          <div class="panel-heading">Mon</div>
                        </div>
                      </label>
                      <span id="mon-span" class="custom-error-span day_error">Required</span>
                  </div>
                  <div class="col-md-1 radio-div">
                    <label class="tue-lbl">
                      <input type="radio" name="weekDay" value="tue" class="card-input-element" />
                      <div class="panel panel-default card-input">
                        <div class="panel-heading">Tue</div>
                      </div>
                    </label>
                    <span id="tue-span" class="custom-error-span day_error">Required</span>
                  </div>
                  <div class="col-md-1 radio-div">
                    <label class="wed-lbl">
                      <input type="radio" name="weekDay" value="wed" class="card-input-element" />
                        <div class="panel panel-default card-input">
                          <div class="panel-heading">Wed</div>
                        </div>
                    </label>
                    <span id="wed-span" class="custom-error-span day_error">Required</span>
                  </div>
                  <div class="col-md-1 radio-div">
                    <label class="thu-lbl">
                      <input type="radio" name="weekDay" value="thu" class="card-input-element" />
                        <div class="panel panel-default card-input">
                          <div class="panel-heading">Thu</div>
                        </div>
                    </label>
                    <span id="thu-span" class="custom-error-span day_error">Required</span>
                  </div>
                  <div class="col-md-1 radio-div">
                    <label class="fri-lbl">
                      <input type="radio" name="weekDay" value="fri" class="card-input-element" />
                        <div class="panel panel-default card-input">
                          <div class="panel-heading">Fri</div>
                        </div>
                    </label>
                    <span id="fri-span" class="custom-error-span day_error">Required</span>
                  </div>
                  <div class="col-md-1 radio-div">
                    <label class="sat-lbl">
                      <input type="radio" name="weekDay" value="sat" class="card-input-element" />
                        <div class="panel panel-default card-input">
                          <div class="panel-heading">Sat</div>
                        </div>
                    </label>
                    <span id="sat-span" class="custom-error-span day_error">Required</span>
                  </div>
                  <div class="col-md-1 radio-div">
                    <label class="sun-lbl">
                      <input type="radio" name="weekDay" value="sun" class="card-input-element" />
                        <div class="panel panel-default card-input">
                          <div class="panel-heading">Sun</div>
                        </div>
                    </label>
                    <span id="sun-span" class="custom-error-span day_error">Required</span>
                  </div>
              </div>
              <div class="form-group mon-div custom-slot active vet-profile-hide">
                <div class="" id="mon-buildyourform">
                    
                  <% if(typeof responseData==='object' && typeof responseData.VetPracticeTimeBlocks==='object' ) { 
                    for (let i=0, cnt = 1; i < responseData.VetPracticeTimeBlocks.length; i++ ) {
                      if(responseData.VetPracticeTimeBlocks[i].day === "monday") { %>
                      <div class="form-group mon_fieldwrapper" id="field1">
                        <label for="monday1" class="">Slot </label>&nbsp;<label class="slot-id mon"><%= cnt %></label>&nbsp;<label class="mon-invalid-time time-error"></label><br>
                        <div class="col-sm-2 form-inline custom-input-padding from-time">
                            <input type="hidden" name="monday[]" class="form-control" value="<%= responseData.VetPracticeTimeBlocks[i].id %>">
                            <input type="time" name="monday[]" id="monday1" class="form-control mondayTime typeTime fromtime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].from_time).format('HH:mm') %>">
                          </div>
                        <div class="col-sm-2 form-inline to-time custom-to" data-idx="<%= cnt %>">
                            <input type="time" name="monday[]" id="monday2" class="form-control mondayTime typeTime totime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].to_time).format('HH:mm') %>">
                          </div>
                        <input type="button" class="remove" value="-" style="position: absolute;margin-left: -2%;padding-top: 4px;padding-bottom: 4px;"><br><br>
                      </div>
                  <% cnt++ }}} %>
                </div>
                <div class="form-group row vet-profile-hide">
                  <div class="col-sm-6">
                    <input type="button" value="Add a field" class="add" id="mon-add" />
                  </div>
                </div>
              </div>
              <div class="form-group tue-div custom-slot">
                <div class="" id="tue-buildyourform">
                  <% if(typeof responseData==='object' && typeof responseData.VetPracticeTimeBlocks==='object' ) { 
                    for (let i=0, cnt = 1; i < responseData.VetPracticeTimeBlocks.length; i++ ) {
                      if(responseData.VetPracticeTimeBlocks[i].day === "tuesday") { %>
                        <div class="form-group tue_fieldwrapper" id="field1">
                          <label for="tuesday1" class="">Slot </label>&nbsp;<label class="slot-id tue"><%= cnt %></label>&nbsp;<label class="tue-invalid-time time-error"></label><br>
                          <div class="col-sm-2 form-inline custom-input-padding from-time">
                              <input type="hidden" name="tuesday[]" class="form-control" value="<%= responseData.VetPracticeTimeBlocks[i].id %>">
                              <input type="time" name="tuesday[]" id="tuesday1" class="form-control tuesdayTime tueTime typeTime fromtime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].from_time).format('HH:mm') %>">
                          </div>
                          <div class="col-sm-2 form-inline to-time custom-to" data-idx="<%= cnt %>">
                              <input type="time" name="tuesday[]" id="tuesday2" class="form-control tuesdayTime tueTime typeTime totime" value="<%= moment(responseData.VetPracticeTimeBlocks[i].to_time).format('HH:mm') %>">
                          </div>
                          <input type="button" class="remove" value="-" style="position: absolute;margin-left: -2%;padding-top: 4px;padding-bottom: 4px;"><br><br>
                        </div>
                  <% cnt++ }}} %>
                </div>
                <div class="form-group row">
                  <div class="col-sm-6">
                    <input type="button" value="Add a field" class="add" id="tue-add" />
                  </div>
                </div>
              </div>
